TC=> O(N)
SC= O(1)

class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
      int slow =0;
      if(nums.empty()){
        return 0;
      }
      
      for(int fast=0; fast<nums.size(); fast++){
        if(nums[fast] != nums[slow]){
            slow++;
            nums[slow] = nums[fast]; 
        }
      }
      return slow+1;   
    }
};

-------------------------------------------------------------------------------------------
class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int index = 1;
        for(int i = 1; i<nums.size(); i++){
            if(nums[i] != nums[i-1]){
                nums[index]= nums[i];
                index++;
            }
        }
        return index;
        
    }
}
